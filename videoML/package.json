{
    "name": "videoML",
    "description": "Please enter a brief description here",
    "version": "0.0.1",
    "files": [
        "bin",
        "out",
        "src"
    ],
    "type": "module",
    "scripts": {
        "build": "tsc -b tsconfig.src.json && node esbuild.mjs",
        "watch": "concurrently -n tsc,esbuild -c blue,yellow \"tsc -b tsconfig.src.json --watch\" \"node esbuild.mjs --watch\"",
        "lint": "eslint src --ext ts",
        "langium:generate": "langium generate",
        "langium:generate:production": "langium generate --mode=production",
        "langium:watch": "langium generate --watch",
        "vscode:prepublish": "npm run build && npm run lint",
        "build:web": "npm run build",
        "bundle": "vite build",
        "bundle:serve": "http-server ./dist --port 5175",
        "dev": "vite",
        "dev:debug": "vite --debug --force",
        "serve": "npm run dev",
        "test": "vitest run",
        "cli:info": "node ./bin/cli",
        "cli": "node ./bin/cli generate --destination ../demo/generated/",
        "prepare:package": "npm run langium:generate:production && npm run build && npm run bundle"
    },
    "dependencies": {
        "@codingame/monaco-vscode-editor-service-override": "~3.2.3",
        "@codingame/monaco-vscode-keybindings-service-override": "~3.2.3",
        "@emotion/react": "^11.14.0",
        "@vitejs/plugin-react": "^4.3.4",
        "chalk": "~5.3.0",
        "commander": "~11.0.0",
        "langium": "~3.3.0",
        "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~3.2.3",
        "monaco-editor-wrapper": "~4.0.2",
        "monaco-languageclient": "~8.1.1",
        "react": "^19.0.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^19.0.0",
        "react-icons": "^5.4.0",
        "vscode": "npm:@codingame/monaco-vscode-api@~3.2.3",
        "vscode-languageclient": "~9.0.1",
        "vscode-languageserver": "~9.0.1",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@codingame/esbuild-import-meta-url-plugin": "~1.0.2",
        "@types/node": "^18.19.70",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "@types/vscode": "~1.67.0",
        "@typescript-eslint/eslint-plugin": "~7.3.1",
        "@typescript-eslint/parser": "~7.3.1",
        "concurrently": "~8.2.1",
        "esbuild": "~0.20.2",
        "eslint": "~8.57.0",
        "http-server": "~14.1.1",
        "langium-cli": "~3.3.0",
        "typescript": "~5.1.6",
        "vite": "~5.2.7",
        "vitest": "~1.4.0"
    },
    "volta": {
        "node": "18.19.1",
        "npm": "10.2.4"
    },
    "displayName": "videoML",
    "engines": {
        "vscode": "^1.67.0",
        "node": ">=18.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "video-ml",
                "aliases": [
                    "videoML",
                    "video-ml"
                ],
                "extensions": [
                    ".vml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "video-ml",
                "scopeName": "source.video-ml",
                "path": "syntaxes/video-ml.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:video-ml"
    ],
    "main": "./out/extension/main.cjs",
    "bin": {
        "video-ml-cli": "./bin/cli.js"
    }
}
